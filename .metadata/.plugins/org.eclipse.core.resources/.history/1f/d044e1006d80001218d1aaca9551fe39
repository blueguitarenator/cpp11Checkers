#include <Checkers/Checker.h>
#include <Checkers/Square.h>
#include <memory>
#include <gtest/gtest.h>

using namespace std;

TEST(CheckerTest, asdf)
{
	shared_ptr<Square> sq = make_shared<Square>(1);

	Checker c(sq);
	EXPECT_EQ(20, c.work(2, 18));

}



