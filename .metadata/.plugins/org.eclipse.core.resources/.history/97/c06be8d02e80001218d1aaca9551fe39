#include "Checker.h"
#include <memory>

using namespace std;

Checker::Checker(Square* square)
:m_square(square)
{
	unique_ptr<int> x = new int(1);

}

Checker::~Checker()
{
}

int Checker::work(int x, int y)
{
	return x + y;
}
